{
  "version": "0.0.0",
  "private": true,
  "name": "templates",
  "description": "templates: lib/libs/app",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:watch": "umi-test -w",
    "test:coverage": "umi-test --coverage",
    "clean": "lerna clean",
    "changelog": "lerna-changelog",
    "publish": "npm test && npm run changelog && npm run build && lerna publish -m \"chore: publish\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2snail/templates.git"
  },
  "keywords": [
    "2snail",
    "template"
  ],
  "author": "zhangaz1",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/2snail/templates/issues"
  },
  "homepage": "https://github.com/2snail/templates#readme",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.10",
    "execa": "^4.0.3",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lerna": "^3.22.1",
    "lerna-changelog": "^1.0.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
}
